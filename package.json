{
  "name": "anime365wrapper",
  "version": "1.0.0",
  "description": "Wrapper anime365.ru (smotret-anime) for Node.JS",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "prepare": "npm run build",
    "start": "node dist/cjs/index.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thedvxchsquad/anime365wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/thedvxchsquad/anime365wrapper/issues"
  },
  "homepage": "https://github.com/thedvxchsquad/anime365wrapper#readme",
  "keywords": [
    "anime",
    "typescript",
    "api",
    "wrapper",
    "smotret-anime"
  ],
  "author": "dvxchsquad",
  "contributors": [
    "ragedrugg"
  ],
  "license": "MIT",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  }
}
